<div style="min-height:100%;">
  <section data-background-color="black">
      <div class="container">
        <div class="row">
            <h3>Wallet commands</h3>
            <% data.wallet_commands.commands.each do |command| %>
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="border-bottom: 1px dashed grey;">
                    <h4>
                      <b><%= command.command %></b>
                      <% if command.parameters != "" %>
                        <%= command.parameters %>
                      <% end %>
                    </h4>
                    <p><%= command.description %></p>
                    <p>
                      <% if command.unlock == "Y" %>
                        Requires unlocked wallet
                      <% else %>
                        Wallet unlock unneccessarry
                      <% end %>
                    </p>
                </div>
            <% end %>
        </div>

        <div class="row" style="padding-top:50px;">
            <h3>Mining commands</h3>
            <% data.mining_commands.commands.each do |command| %>
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="border-bottom: 1px dashed grey;">
                    <h4>
                      <b><%= command.command %></b>
                      <% if command.parameters != "" %>
                        <%= command.parameters %>
                      <% end %>
                    </h4>
                    <p><%= command.description %></p>
                    <p>
                      <% if command.unlock == "Y" %>
                        Requires unlocked wallet
                      <% else %>
                        Wallet unlock unneccessarry
                      <% end %>
                    </p>
                </div>
            <% end %>
        </div>

        <div class="row" style="padding-top:50px;">
            <h3>Network commands</h3>
            <% data.network_commands.commands.each do |command| %>
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="border-bottom: 1px dashed grey;">
                    <h4>
                      <b><%= command.command %></b>
                      <% if command.parameters != "" %>
                        <%= command.parameters %>
                      <% end %>
                    </h4>
                    <p><%= command.description %></p>
                    <p>
                      <% if command.unlock == "Y" %>
                        Requires unlocked wallet
                      <% else %>
                        Wallet unlock unneccessarry
                      <% end %>
                    </p>
                </div>
            <% end %>
        </div>
      </div>
  </section>
</div>
